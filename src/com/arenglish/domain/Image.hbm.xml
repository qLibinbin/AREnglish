<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.arenglish.domain">
	<class name="Image" table="are_image" lazy="true">
		<id name="id" column="are_image_id">
			<generator class="assigned"></generator>
		</id>

		<property name="tag" column="are_image_tag"></property>
		<property name="path" column="are_image_path"></property>
		<property name="undefinedName" column="are_image_undefined_name"></property>
		
		
	</class>
</hibernate-mapping>